<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name ="_04com.nag.Primary_Key_Auto_Genertors_Increment.Student" table ="student1" schema="system">
		<id name="id" column="std_id">
			<generator class="increment"/>
		</id>
		<property name="name" column="std_name"/>
		<property name="email" column="std_email"/>
		<property name="marks" column="std_marks"/>		
	</class>

</hibernate-mapping>